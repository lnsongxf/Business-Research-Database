i","family":"Yang","affiliation":[]}],"member":"109","reference":[{"issue":"4","key":"B2","first-page":"119","volume":"27","author":"Bigham P.","year":"1986","journal-title":"Production and Inventory Management"},{"key":"B3","DOI":"10.1287\/opre.1070.0478"},{"key":"B4","DOI":"10.1287\/opre.1040.0127"},{"key":"B5","DOI":"10.1287\/opre.1050.0229"},{"key":"B6","DOI":"10.1016\/S0377-2217(99)00314-8"},{"key":"B7","DOI":"10.1080\/09537289408919532"},{"key":"B8","DOI":"10.1287\/mnsc.43.2.184"},{"key":"B9","DOI":"10.1287\/mnsc.25.4.374"},{"key":"B10","DOI":"10.1137\/0117052"},{"key":"B11","DOI":"10.1287\/mnsc.17.7.411"},{"key":"B12","DOI":"10.1111\/j.1540-5915.2001.tb00966.x"},{"key":"B13","author":"Scarf H.","first-page":"196","year":"1960","volume-title":"Mathematical Methods in the Social Sciences"},{"key":"B14","DOI":"10.1287\/opre.1060.0357"},{"key":"B15","DOI":"10.1287\/opre.1030.0104"},{"key":"B16","DOI":"10.1287\/opre.44.6.1013"},{"key":"B17","DOI":"10.1137\/0114086"},{"key":"B18","DOI":"10.1287\/mnsc.11.5.525"},{"key":"B19","DOI":"10.1287\/opre.1060.0386"},{"key":"B20","author":"Yu Y.","year":"2012","journal-title":"Oper. Res."}],"container-title":["Operations Research"],"original-title":[],"deposited":{"date-parts":[[2016,11,8]],"date-time":"2016-11-08T17:14:25Z","timestamp":1478625265000},"score":1.0,"subtitle":[],"short-title":[],"issued":{"date-parts":[[2012,8]]},"references-count":19,"alternative-id":["10.1287\/opre.1110.1033"],"URL":"http:\/\/dx.doi.org\/10.1287\/opre.1110.1033","relation":null,"ISSN":["0030-364X","1526-5463"],"issn-type":[{"value":"0030-364X","type":"print"},{"value":"1526-5463","type":"electronic"}],"subject":["Management Science and Operations Research","Computer Science Applications"]},{"indexed":{"date-parts":[[2017,3,30]],"date-time":"2017-03-30T21:39:29Z","timestamp":1490909969733},"publisher":"Institute for Operations Research and the Management Sciences (INFORMS)","issue":"4","content-domain":{"domain":[],"crossmark-restriction":fals