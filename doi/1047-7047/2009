ad","affiliation":[]}],"member":"109","reference":[{"key":"B1","DOI":"10.1257\/0002828043052169"},{"key":"B2","DOI":"10.1111\/1467-937X.00168"},{"key":"B3","author":"Brock W. A.","volume":"5","first-page":"3463","year":"2001","volume-title":"Handbook of Econometrics"},{"key":"B4","DOI":"10.1145\/641205.641208"},{"issue":"3","key":"B5","first-page":"399","volume":"29","author":"Brown S. A.","year":"2005","journal-title":"MIS Quart."},{"key":"B6","DOI":"10.1016\/j.jue.2007.08.002"},{"key":"B7","DOI":"10.1162\/108118000568967"},{"key":"B8","DOI":"10.2307\/256603"},{"issue":"7","key":"B9","first-page":"39","volume":"1","author":"Chen W.","year":"2004","journal-title":"IT Soc."},{"key":"B11","DOI":"10.1016\/S0047-2727(01)00098-6"},{"key":"B12","DOI":"10.1002\/(SICI)1520-6653(200021)14:2<17::AID-DIR2>3.0.CO;2-E"},{"key":"B14","DOI":"10.2307\/2946686"},{"key":"B15","DOI":"10.1086\/344399"},{"key":"B16","DOI":"10.1162\/003355300554674"},{"key":"B17","DOI":"10.1177\/107769900007700306"},{"key":"B18","DOI":"10.1145\/240483.240490"},{"issue":"3","key":"B19","first-page":"97","volume":"18","author":"Hong W.","year":"2002","journal-title":"J. MIS"},{"key":"B20","DOI":"10.1162\/003355300555060"},{"key":"B22","DOI":"10.1108\/10662249710171814"},{"key":"B23","DOI":"10.1145\/240483.240493"},{"key":"B24","DOI":"10.1287\/isre.10.4.287"},{"key":"B25","DOI":"10.1016\/S0268-4012(98)00047-4"},{"key":"B26","DOI":"10.1177\/009365001028004007"},{"key":"B27","author":"Loury G.","year":"1977","volume-title":"Women, Minorities, and Employment Discrimination"},{"key":"B29","DOI":"10.2307\/2298123"},{"issue":"2","key":"B30","first-page":"281","volume":"67","author":"Massey D. S.","year":"1988","journal-title":"Soc. Forces"},{"key":"B31","DOI":"10.5210\/fm.v7i11.1003"},{"key":"B32","DOI":"10.1111\/1468-2257.00215"},{"key":"B34","DOI":"10.1016\/j.jdeveco.2003.03.003"},{"issue":"1","key":"B35","first-page":"35","volume":"15","author":"Niles S.","year":"2003","journal-title":"Urban and Regional Infor