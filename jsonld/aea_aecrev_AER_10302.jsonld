[{"ex:issue": "2", "ex:abstract": "Drastic changes in central bank operations and monetary institutions", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Paper Money", "ex:url": "http://www.aeaweb.org/aer/ds/april2013/2013pres_ds.zip", "ex:pages": "563-84", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Christopher A. Sims"]}, {"ex:issue": "2", "ex:abstract": "Randomization is commonplace in everyday resource allocation. We", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Designing Random Allocation Mechanisms: Theory and Applications", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20110377_app.pdf", "ex:pages": "585-623", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Eric Budish", "Yeon-Koo Che", "Fuhito Kojima", "Paul Milgrom"]}, {"ex:issue": "2", "ex:abstract": "We estimate a model of strategic voting and quantify the impact it", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Inferring Strategic Voting", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100067_app.pdf", "ex:pages": "624-62", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Kei Kawai", "Yasutora Watanabe"]}, {"ex:issue": "2", "ex:abstract": "Dynamic matching and bargaining games are models of decentralized", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Dynamic Matching and Bargaining Games: A General Approach", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20110333_app.pdf", "ex:pages": "663-89", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Stephan Lauermann"]}, {"ex:issue": "2", "ex:abstract": "Languages differ widely in the ways they encode time. I test the", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "The Effect of Language on Economic Behavior: Evidence from Savings Rates, Health Behaviors, and Retirement Assets", "ex:url": "http://www.aeaweb.org/aer/ds/april2013/20111628_ds.zip", "ex:pages": "690-731", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["M. Keith Chen"]}, {"ex:issue": "2", "ex:abstract": "We model the dynamics of risk premia during crises in asset markets", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Intermediary Asset Pricing", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100714_app.pdf", "ex:pages": "732-70", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Zhiguo He", "Arvind Krishnamurthy"]}, {"ex:issue": "2", "ex:abstract": "We consider a model with on-the-job search where current wages", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Job Selection and Wages over the Business Cycle", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20090968_app.pdf", "ex:pages": "771-803", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Marcus Hagedorn", "Iourii Manovskii"]}, {"ex:issue": "2", "ex:abstract": "We use novel data to study genetic testing among individuals at", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Optimal Expectations and Limited Medical Testing: Evidence from Huntington Disease", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20111522_app.pdf", "ex:pages": "804-30", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Emily Oster", "Ira Shoulson", "E. Ray Dorsey"]}, {"ex:issue": "2", "ex:abstract": "We estimate a spatial model of liquor demand to analyze the impact of", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Public Monopoly and Economic Efficiency: Evidence from the Pennsylvania Liquor Control Board's Entry Decisions", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100822_app.pdf", "ex:pages": "831-62", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Katja Seim", "Joel Waldfogel"]}, {"ex:issue": "2", "ex:abstract": "We provide evidence that increased political influence, arising from", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Commercial Imperialism? Political Influence and Trade during the Cold War", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100514_app.pdf", "ex:pages": "863-96", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Daniel Berger", "William Easterly", "Nathan Nunn", "Shanker Satyanath"]}, {"ex:issue": "2", "ex:abstract": "This paper empirically assesses the incidence and efficiency of", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Assessing the Incidence and Efficiency of a Prominent Place Based Policy", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20110193_app.pdf", "ex:pages": "897-947", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Matias Busso", "Jesse Gregory", "Patrick Kline"]}, {"ex:issue": "2", "ex:abstract": "Cross-country labor productivity differences are larger in agriculture", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Selection, Agriculture, and Cross-Country Productivity Differences", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20101022_app.pdf", "ex:pages": "948-80", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["David Lagakos", "Michael E. Waugh"]}, {"ex:issue": "2", "ex:abstract": "Using a new nationally representative dataset, we find minor differences in test outcomes between black and white infants that disappear with a limited set of controls. However, relative to whites, all other races lose substantial ground by age two. Combining our estimates with results in prior literature, we show that a simple model with assortative mating fits our data well, implying that differences in children's environments between racial groups can fully explain gaps in intelligence. If parental ability influences a child's test scores both genetically and through environment, then our findings are less informative and can be reconciled with a wide range of racial differences in inherited intelligence.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Testing for Racial Differences in the Mental Ability of Young Children", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20060380_data.zip", "ex:pages": "981-1005", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Roland G. Fryer", "Steven D. Levitt"]}, {"ex:issue": "2", "ex:abstract": "We analyze the optimal policy of an antitrust authority towards horizontal mergers when merger proposals are endogenous and firms choose among alternative mergers. In our model, the optimal policy of an antitrust authority that seeks to maximize expected consumer surplus imposes a tougher standard on \"larger\" mergers, i.e., those involving firms with a larger pre-merger market share. The optimal policy is a response to a bias in firms' proposal incentives: firms always propose a larger merger when it is better for consumers than a smaller one, but sometimes will propose the larger one even when it is worse for consumers.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Merger Policy with Merger Choice", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20110455_app.pdf", "ex:pages": "1006-33", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Volker Nocke", "Michael D. Whinston"]}, {"ex:issue": "2", "ex:abstract": "The standard assumption that asset demand increases in income and decreases in price has its origin in Arrow's classic model with one risky and one risk free asset, where both are held long, and preferences exhibit decreasing absolute and increasing relative risk aversion. However if one allows shorting of the risk free asset or decreasing relative risk aversion, the risk free asset can not only fail to be a normal good but can be a Giffen good. This behavior can occur even for members of the popular HARA utility family. More generally, Giffen behavior can occur over multiple income ranges.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Inferior Good and Giffen Behavior for Investing and Borrowing", "ex:url": "http://www.aeaweb.org/articles.php?doi=10.1257/aer.103.2.1034", "ex:pages": "1034-53", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Felix Kubler", "Larry Selden", "Xiao Wei"]}, {"ex:issue": "2", "ex:abstract": "Using recent results in the measurement error literature, we show that the official US unemployment rate substantially underestimates the true level of unemployment, due to misclassification errors in the labor force status in the Current Population Survey. During the period from January 1996 to August 2011, the corrected monthly unemployment rates are between 1 and 4.4 percentage points (2.1 percentage points on average) higher than the official rates, and are more sensitive to changes in business cycles. The labor force participation rates, however, are not affected by this correction.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Misclassification Errors and the Underestimation of the US Unemployment Rate", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100815_app.pdf", "ex:pages": "1054-70", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Shuaizhang Feng", "Yingyao Hu"]}, {"ex:issue": "2", "ex:abstract": "The Bagwell and Staiger (1990) theory of cooperative trade agreements predicts new tariffs (i) increase with imports, (ii) increase with the inverse of the sum of the import demand and export supply elasticities, and (iii) decrease with the variance of imports. We find US import policy during 1997-2006 to be consistent with this theory. A one standard deviation increase in import growth, the inverse of the sum of the import demand and export supply elasticity, and the standard deviation of import growth changes the probability that the US imposes an antidumping tariff by 35 percent, by 88 percent, and by -76 percent, respectively.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "Self-Enforcing Trade Agreements: Evidence from Time-Varying Trade Policy", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20100125_data.zip", "ex:pages": "1071-90", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Chad P. Bown", "Meredith A. Crowley"]}, {"ex:issue": "2", "ex:abstract": "We present a theory of charitable fund-raising in which it is costly to solicit donors. We fully characterize the solicitation strategy that maximizes donations net of fund-raising costs. It is optimal for the fund-raiser to target only the \"net contributors\" -- donors who would give more than their solicitation costs. We show that as the income inequality increases, so does the level of the public good, despite a (potentially) non-monotonic fund-raising strategy. This implies that costly fund-raising can provide a novel explanation for the non-neutrality of income redistributions and government grants often found in empirical studies.", "ex:volume": "103", "ex:source": "American Economic Review", "ex:title": "A Theory of Charitable Fund-Raising with Costly Solicitations", "ex:url": "http://www.aeaweb.org/aer/data/april2013/20110665_app.pdf", "ex:pages": "1091-1107", "ex:year": "", "ex:date": "2013", "ex:keyword": [], "ex:creator": ["Alvaro J. Name-Correa", "Huseyin Yildirim"]}]