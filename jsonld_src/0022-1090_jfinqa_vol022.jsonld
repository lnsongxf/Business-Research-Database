[{"e:author": ["Mihir Bhattacharya"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012394", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "1-15", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012394", "e:year": 1987, "e:title": "Price Changes of Related Securities: The Case of Call Options and Stocks", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Laura T. Starks"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012400", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "17-32", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012400", "e:year": 1987, "e:title": "Performance Incentive Fees: An Agency Theoretic Approach", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["David F. Larcker"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012412", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "33-50", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012412", "e:year": 1987, "e:title": "Short-Term Compensation Contracts and Executive Expenditure Decisions: The Case of Commercial Banks", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Seha M. Tinic", "Giovanni Barone-Adesi", "Richard R. West"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012424", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "51-63", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012424", "e:year": 1987, "e:title": "Seasonality in Canadian Stock Prices: A Test of the “Tax-Loss-Selling” Hypothesis", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Malcolm R. Burns"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012436", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "65-77", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012436", "e:year": 1987, "e:title": "New Evidence on the Value Additivity Principle", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Miles Livingston"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012448", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "79-87", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012448", "e:year": 1987, "e:title": "The Delivery Option on Forward Contracts", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Eric C. Chang", "J. Michael Pinegar"], "e:url": "http://journals.cambridge.org/abstract_S002210900001245X", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "89-99", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S002210900001245X", "e:year": 1987, "e:title": "Risk and Inflation", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["John Doukas", "Abdul Rahman"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012461", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "101-108", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012461", "e:year": 1987, "e:title": "Unit Roots Tests: Evidence from the Foreign Exchange Futures Market", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Jonathan M. Karpoff"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012473", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "109-126", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012473", "e:year": 1987, "e:title": "The Relation between Price Changes and Trading Volume: A Survey", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "01"}, {"e:author": ["Lawrence Harris"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012503", "e:volume": "22", "e:abstract": "This paper presents new tests of the mixture of distributions hypothesis. Previous tests examined security prices and volume measured only at daily intervals. Here, differential implications of the hypothesis for transaction data are derived and tested. The new predictions emanate from the assumption that prices and volume evolve at uniform rates in transaction time. The results support this assumption and the mixture of distributions hypothesis in general. In addition, the tests suggest that the daily transaction-count may be a useful instrumental variable for estimating unobserved realizations of stochastic price variances.", "e:keyword": [], "e:pages": "127-141", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012503", "e:year": 1987, "e:title": "Transaction Data Tests of the Mixture of Distributions Hypothesis", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Herb Johnson", "David Shanno"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012515", "e:volume": "22", "e:abstract": "The Monte Carlo method is used to solve for the price of a call when the variance is changing stochastically.", "e:keyword": [], "e:pages": "143-151", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012515", "e:year": 1987, "e:title": "Option Pricing when the Variance Is Changing", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["James N. Bodurtha", "Georges R. Courtadon"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012527", "e:volume": "22", "e:abstract": "This paper tests the ability of the American option pricing model proposed by Parkinson [18] or Mason [14] to explain the pricing of the foreign currency options traded on the Philadelphia Stock Exchange from February 28, 1983 to March 26, 1985. We find that the model underprices out-of-the-money options relative to at-the-money and in-the-money options. This relative underpricing is driven by an underpricing of out-of-the-money call options of short maturity. In addition, the degree of relative mispricing for most categories of options is shown to be a decreasing function of the time to maturity of the options. Longer maturity options appear to trade at similar levels of implied volatility whether they are in, at, or out of the money. Most of these biases appear consistent with the fact that the underlying spot currency rate follows a mixed jump diffusion process as described in [17].", "e:keyword": [], "e:pages": "153-167", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012527", "e:year": 1987, "e:title": "Tests of an American Option Pricing Model on the Foreign Currency Options Market", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Victor L. Bernard", "Thomas J. Frecka"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012539", "e:volume": "22", "e:abstract": "Evidence provided here suggests that investors could have identified, <italic>ex ante</italic>, portfolios that hedge against uncertainty in the prices of three major categories of consumption: food, shelter, and transportation. This finding has implications for the practical importance of multi-period capital asset pricing theories that assume investors can identify such hedge portfolios. The study also provides some surprising evidence about the usefulness of common stocks and commodity futures contracts in hedging against specific price inflation. Certain combinations of common stocks serve as effective hedges, but combinations of commodity futures contracts contribute nothing to hedging ability.", "e:keyword": [], "e:pages": "169-188", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012539", "e:year": 1987, "e:title": "Commodity Contracts and Common Stocks as Hedges against Relative Consumer Price Risk", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Haim Mendelson"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012540", "e:volume": "22", "e:abstract": "This paper studies the impact of market consolidation or fragmentation on its performance, examining four alternative models of exchange: a consolidated clearing house, fragmented clearing houses, a monopoly dealer market, and an interdealer market. The effects of the market mechanism on the expected quantity traded, the price variance faced by individual traders, the quality of market price signals, the expected gains from trade, and the exchange implementation costs are studied.", "e:keyword": [], "e:pages": "189-207", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012540", "e:year": 1987, "e:title": "Consolidation, Fragmentation, and Market Performance", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Gary W. Emery"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012552", "e:volume": "22", "e:abstract": "This paper develops a positive theory of trade credit based on its use as a financial response to deterministic variations in demand. The operating alternatives to trade credit, which include the use of storage or additional capacity, are modeled using results from the peak-load pricing literature. The paper shows that the extension of credit partitions the buyer's inventory cost and permits specialization at incurring the components of this cost. This specialization is economical when the seller has an advantage at incurring the financial cost <italic>and</italic> does not have an advantage at incurring the operating cost of accommodating variable demand. Conditions that provide these necessary and sufficient cost relationships are described. The paper also shows that a reduction in costs rather than an increase in revenues is the source of both the buyer's and seller's increase in wealth.", "e:keyword": [], "e:pages": "209-225", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012552", "e:year": 1987, "e:title": "An Optimal Financial Response to Variable Demand", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Michael C. Ehrhardt"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012564", "e:volume": "22", "e:abstract": "The primary objective of this paper is to derive a multi-factor equilibrium model using a mean-variance approach. The results of this derivation provide greater insight into the nature of the resulting factors than does APT. There are several important implications for empirical tests of any <italic>a priori</italic> defined multi-factor model.", "e:keyword": [], "e:pages": "227-236", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012564", "e:year": 1987, "e:title": "A Mean-Variance Derivation of a Multi-Factor Equilibrium Model", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Tony van Zijl"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012576", "e:volume": "22", "e:abstract": "This paper reexamines and extends the work of Ben Horim and Levy [1], which argued that risk decomposition should be based on standard deviation rather than on variance. Their analysis showed that decomposition of variance is wrong when β < 0 and that, in general, this procedure produces incorrect estimates of undiversifiable risk. This paper shows that these conclusions also apply to the no-risk-free asset extended CAPM if risk is adjusted for the unavoidable risk associated with the global minimum variance portfolio.", "e:keyword": [], "e:pages": "237-247", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012576", "e:year": 1987, "e:title": "Risk Decomposition: Variance or Standard Deviation—A Reexamination and Extension", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Keith M. Howe", "Harvey Lapan"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012588", "e:volume": "22", "e:abstract": "Our paper extends prior work on abandonment and replacement policies by analyzing the effects of both the Darby and the Fisher interest rate hypotheses. In most of the new cases developed, the effect of increased inflation on economic life is ambiguous. In the abandonment problem, we find a greater tendency under the Fisher hypothesis than under the Darby hypothesis for increased inflation to result in an extension of economic life. In contrast, for the replacement problem, there is a greater tendency under the Fisher hypothesis (than under Darby) for inflation to shorten the asset holding period.", "e:keyword": [], "e:pages": "249-258", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012588", "e:year": 1987, "e:title": "Inflation and Asset Life: The Darby versus the Fisher Effect", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "02"}, {"e:author": ["Avi Bick"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012618", "e:volume": "22", "e:abstract": "We construct a simple economy with consumption only at the final date in which we “endogenize” the stochastic behavior of prices assumed in the Black-Scholes model. Certain preferences (constant proportional risk aversion) and beliefs are shown to be sufficient and necessary, in certain respects, for the existence of such an equilibrium. The analysis is then generalized to a continuous-consumption framework, in which we embed the Merton proportional dividend model.", "e:keyword": [], "e:pages": "259-275", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012618", "e:year": 1987, "e:title": "On the Consistency of the Black-Scholes Model with a General Equilibrium Framework", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Herb Johnson"], "e:url": "http://journals.cambridge.org/abstract_S002210900001262X", "e:volume": "22", "e:abstract": "Using an intuitive approach that also provides new intuition concerning the Black and Scholes equation, this paper extends the results of Johnson and Stulz to the pricing of options on the minimum or the maximum of <italic>several</italic> risky assets.", "e:keyword": [], "e:pages": "277-283", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S002210900001262X", "e:year": 1987, "e:title": "Options on the Maximum or the Minimum of Several Assets", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Haim Levy", "Azriel Levy"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012631", "e:volume": "22", "e:abstract": "Most research dealing with portfolio selection under uncertain inflation is carried out by assuming either one of the following two approximations: a linear or a quadratic approximation. In this paper, we analyze the general case, namely assume that the nominal return is the product of the real return and one plus the rate of inflation. We demonstrate that the general analysis leads to the following results that are not found in the two approximations: (1) even if we assume that nominal returns are independent of inflation, the nominal and real efficient sets will not necessarily coincide. Mean-Variance (M-V) analysis leads to a nominal efficient set, that is, a subset of the real M-V efficient set, whereas the opposite holds assuming investors maximize expected utility of real wealth. (2) Similar results are obtained when real returns are independent of inflation (the Fisher hypothesis). Assuming normality of nominal returns, we derive the CAPM in real terms or its zero beta counterpart.", "e:keyword": [], "e:pages": "285-297", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012631", "e:year": 1987, "e:title": "Equilibrium under Uncertain Inflation: A Discrete Time Approach", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["David S. Kidwell", "Eric H. Sorensen", "John M. Wachowicz"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012643", "e:volume": "22", "e:abstract": "This paper examines the demand for municipal bond insurance in the context of a competitive signaling equilibrium model. The study compares the pricing of new bond issues that are insured to similar issues that are not insured. The results indicate that issuers who purchase bond insurance, on average, are able to reduce their new issue borrowing cost more than enough to offset the cost of the insurance premium. Furthermore, the net benefit to the issuer increases as the underlying credit quality of the bond declines.", "e:keyword": [], "e:pages": "299-313", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012643", "e:year": 1987, "e:title": "Estimating the Signaling Benefits of Debt Insurance: The Case of Municipal Bonds", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Tim S. Campbell", "William A. Kracaw"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012655", "e:volume": "22", "e:abstract": "This paper investigates the impact of managerial hedging on shareholder wealth when managers are able to choose the level of effort they expend in managing firms' investments. We demonstrate that shareholders will prefer managers to hedge observable unsystematic risks because they expect that this will induce managers to be more productive. We begin with the case where the risk being hedged is independent of managerial effort. In this case, we show that if shareholders are able to adjust incentive contracts either in anticipation of hedging or after observing hedging, but before managers expend effort, then they will benefit from that hedging. When the insurable risk is also dependent on managerial effort, then we have what we term an “embedded moral hazard” problem. In this case, the optimal contract may entail either over or under insurance by the manager, relative to that preferred by shareholders.", "e:keyword": [], "e:pages": "315-328", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012655", "e:year": 1987, "e:title": "Optimal Managerial Incentive Contracts and the Value of Corporate Insurance", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Joseph P. Ogden"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012667", "e:volume": "22", "e:abstract": "This paper argues that an aggregate preferred habitat for investors exists on (or about) the last day of the calendar month, due to standardizations in the nation's payments system resulting in a concentrated flow of funds on this date. Thus, equilibrium yield discounts are predicted for securities maturing on such dates. Empirical tests on monthly and daily Treasury bill data support the principal hypothesis, as well as several ancillary hypotheses. The results have implications for Treasury debt management, for the short-term cash and debt management practices of businesses and banks, and for the empirical estimation of daily, weekly, or monthly return premiums on risky securities.", "e:keyword": [], "e:pages": "329-343", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012667", "e:year": 1987, "e:title": "The End of the Month as a Preferred Habitat: A Test of Operational Efficiency in the Money Market", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["April Klein", "James Rosenfeld"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012679", "e:volume": "22", "e:abstract": "This paper presents evidence that the mean-adjusted returns and raw-market returns models are misspecified when the event under investigation occurs during either bull or bear markets. To demonstrate this phenomenon, simulation techniques as well as an actual event are employed to examine the reliability of four different return-generating models. When the event occurs during a bull (bear) market, both the mean-adjusted and raw-market returns models produce upwardly (downwardly) biased positive (negative) abnormal returns. This results in statistically significant cumulative abnormal returns over selected preevent and postevent intervals. In contrast, both the market-adjusted and single-index models show far less evidence of any unusual price activity over these same intervals.", "e:keyword": [], "e:pages": "345-351", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012679", "e:year": 1987, "e:title": "The Influence of Market Conditions on Event-Study Residuals", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Meir Statman"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012680", "e:volume": "22", "e:abstract": "We show that a well-diversified portfolio of randomly chosen stocks must include at least 30 stocks for a borrowing investor and 40 stocks for a lending investor. This contradicts the widely accepted notion that the benefits of diversification are virtually exhausted when a portfolio contains approximately 10 stocks. We also contrast our result with the levels of diversification found in studies of individuals' portfolios.", "e:keyword": [], "e:pages": "353-363", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012680", "e:year": 1987, "e:title": "How Many Stocks Make a Diversified Portfolio?", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Hal Heaton"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012692", "e:volume": "22", "e:abstract": "This paper demonstrates that the impact of the existing tax law is not uniform across projects with different variances of payoffs. A bias exists against projects with greater uncertainty of payoffs, which leads to an underinvestment in high risk projects. The bias against higher variance projects offers a theoretical justification for such tax incentives as the research and development tax credit.", "e:keyword": [], "e:pages": "365-371", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012692", "e:year": 1987, "e:title": "On the Bias of the Corporate Tax against High-Risk Projects", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Jack S. K. Chang", "Latha Shanker"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012709", "e:volume": "22", "e:abstract": "This paper points out an error and implications of the error in the model of hedging effectiveness proposed by Howard and D'Antonio (1). The error would lead to ambiguous results if the model were used in practical applications to select the best hedging instrument. This paper proposes a new measure of hedging effectiveness that eliminates the error in the original model and resolves the ambiguity.", "e:keyword": [], "e:pages": "373-376", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012709", "e:year": 1987, "e:title": "A Risk-Return Measure of Hedging Effectiveness: A Comment", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Charles T. Howard", "Louis J. D'Antonio"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012710", "e:volume": "22", "e:abstract": "In this reply, we point out that Chang and Shankar's measure of hedging performance, which they label HE<sub>1</sub>, is not an adequate measure. We describe an alternative measure, labeled HBS, which has a number of desirable <italic>ex ante</italic> and <italic>ex post</italic> statistical properties.", "e:keyword": [], "e:pages": "377-381", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012710", "e:year": 1987, "e:title": "A Risk-Return Measure of Hedging Effectiveness: A Reply", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "03"}, {"e:author": ["Ivan E. Brick", "Lawrence Fisher"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012746", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "383-399", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012746", "e:year": 1987, "e:title": "Effects of Classifying Equity or Debt on the Value of the Firm under Tax Asymmetry", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["Nai-Fu Chen", "Thomas E. Copeland", "David Mayers"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012758", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "401-417", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012758", "e:year": 1987, "e:title": "A Comparison of Single and Multifactor Portfolio Performance Methodologies", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["Louis O. Scott"], "e:url": "http://journals.cambridge.org/abstract_S002210900001276X", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "419-438", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S002210900001276X", "e:year": 1987, "e:title": "Option Pricing when the Variance Changes Randomly: Theory, Estimation, and an Application", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["Ehud I. Ronn"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012771", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "439-466", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012771", "e:year": 1987, "e:title": "A New Linear Programming Approach to Bond Portfolio Management", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["Anthony Saunders", "Michael Smirlock"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012783", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "467-482", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012783", "e:year": 1987, "e:title": "Intra- and Interindustry Effects of Bank Securities Market Activities: The Case of Discount Brokerage", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["David R. Peterson"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012795", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "483-494", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012795", "e:year": 1987, "e:title": "Security Price Reactions to Initial Reviews of Common Stock by the Value Line Investment Survey", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["Bill McDonald"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012801", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "495-504", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012801", "e:year": 1987, "e:title": "Event Studies and Systems Methods: Some Additional Evidence", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}, {"e:author": ["George M. Frankfurter", "Christopher G. Lamoureux"], "e:url": "http://journals.cambridge.org/abstract_S0022109000012813", "e:volume": "22", "e:abstract": "", "e:keyword": [], "e:pages": "505-511", "e:issn": "0022-1090", "e:doi": "http://journals.cambridge.org/abstract_S0022109000012813", "e:year": 1987, "e:title": "The Relevance of the Distributional Form of Common Stock Returns to the Construction of Optimal Portfolios", "e:journal": "Journal of Financial and Quantitative Analysis", "e:issue": "04"}]