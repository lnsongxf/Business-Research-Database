[{"e:issn": "2162-9730", "e:volume": "24", "e:url": "http://aisel.aisnet.org/misq/vol24/iss4/1", "e:keyword": [], "e:author": ["Allen S. Lee"], "e:doi": "http://aisel.aisnet.org/misq/vol24/iss4/1", "e:year": 2000, "e:title": "EDITORâ€™S COMMENTS Volume 24 Iss. 4", "e:journal": "Management Information Systems Quarterly", "e:issue": "4"}, {"e:issn": "2162-9730", "e:volume": "23", "e:url": "http://aisel.aisnet.org/misq/vol23/iss1/1", "e:keyword": [], "e:author": ["Kalle Lyytinen"], "e:doi": "http://aisel.aisnet.org/misq/vol23/iss1/1", "e:year": 1999, "e:title": "Empirical Research in Information Systems: On the Relevance of Practice in Thinking of IS Research", "e:journal": "Management Information Systems Quarterly", "e:issue": "1"}, {"e:issn": "2162-9730", "e:volume": "23", "e:url": "http://aisel.aisnet.org/misq/vol23/iss1/4", "e:keyword": [], "e:author": ["Alan Dennis", "Jay Aronson"], "e:doi": "http://aisel.aisnet.org/misq/vol23/iss1/4", "e:year": 1999, "e:title": "Structuring Time and Task in Electronic Brainstorming", "e:journal": "Management Information Systems Quarterly", "e:issue": "1"}, {"e:issn": "2162-9730", "e:volume": "23", "e:url": "http://aisel.aisnet.org/misq/vol23/iss1/2", "e:keyword": [], "e:author": ["Geoff Walsham", "Sundeep Sahay"], "e:doi": "http://aisel.aisnet.org/misq/vol23/iss1/2", "e:year": 1999, "e:title": "GIS for District-Level Administration in India: Problems and Opportunities", "e:journal": "Management Information Systems Quarterly", "e:issue": "1"}, {"e:issn": "2162-9730", "e:volume": "23", "e:url": "http://aisel.aisnet.org/misq/vol23/iss1/3", "e:keyword": [], "e:author": ["Heinz Klein", "Michael Myers"], "e:doi": "http://aisel.aisnet.org/misq/vol23/iss1/3", "e:year": 1999, "e:title": "A Set of Principles for Conducting and Evaluating Interpretive Field Studies in Information Systems", "e:journal": "Management Information Systems Quarterly", "e:issue": "1"}, {"e:issn": "2162-9730", "e:volume": "24", "e:url": "http://aisel.aisnet.org/mis